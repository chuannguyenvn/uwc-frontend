.navigation-bar {
    justify-content: space-around;
    align-self: flex-start;
    align-items: center;
    flex-shrink: 0;
}

.desktop.navigation-bar {
    width: 104px;
    height: 100%;
    padding-right: 2px;
    margin-right: -3px;
}

.mobile.navigation-bar {
    width: 100%;
    height: 100px;
    flex-direction: row;
    margin-top: -3px;
    padding-top: 5px;
}

.panel {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}
